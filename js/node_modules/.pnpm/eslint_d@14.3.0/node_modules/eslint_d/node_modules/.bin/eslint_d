#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/ACER/projects/adventofcode/2015/js/node_modules/.pnpm/eslint_d@14.3.0/node_modules/eslint_d/bin/node_modules:/mnt/c/Users/ACER/projects/adventofcode/2015/js/node_modules/.pnpm/eslint_d@14.3.0/node_modules/eslint_d/node_modules:/mnt/c/Users/ACER/projects/adventofcode/2015/js/node_modules/.pnpm/eslint_d@14.3.0/node_modules:/mnt/c/Users/ACER/projects/adventofcode/2015/js/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/ACER/projects/adventofcode/2015/js/node_modules/.pnpm/eslint_d@14.3.0/node_modules/eslint_d/bin/node_modules:/mnt/c/Users/ACER/projects/adventofcode/2015/js/node_modules/.pnpm/eslint_d@14.3.0/node_modules/eslint_d/node_modules:/mnt/c/Users/ACER/projects/adventofcode/2015/js/node_modules/.pnpm/eslint_d@14.3.0/node_modules:/mnt/c/Users/ACER/projects/adventofcode/2015/js/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/eslint_d.js" "$@"
else
  exec node  "$basedir/../../bin/eslint_d.js" "$@"
fi
